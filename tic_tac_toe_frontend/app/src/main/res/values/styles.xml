<resources>
    <!-- PUBLIC_INTERFACE -->
    <!-- Tic Tac Toe cell style: large, clean, compatible attributes only -->
    <style name="TicCell">
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">100dp</item>
        <item name="android:textSize">36sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#111827</item>
        <item name="android:padding">0dp</item>
        <item name="android:foreground">?android:attr/selectableItemBackgroundBorderless</item>
        <item name="android:backgroundTint">#ECEFF5</item>
    </style>
</resources>
